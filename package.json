{
  "name": "vue-histogram-slider",
  "description": "Range slider with histogram for vue.js",
  "version": "0.3.8",
  "main": "dist/histogram-slider.umd.min.js",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/oguzhaninan/vue-histogram-slider.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "slider",
    "range-slider",
    "histogram",
    "histogram-slider"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --name histogram-slider --entry ./src/lib/HistogramSlider.vue --target lib",
    "prepublishOnly": "yarn lint && yarn build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "d3-array": "^3.0.1",
    "d3-brush": "^3.0.0",
    "d3-scale": "^4.0.0",
    "d3-selection": "^1.4.0",
    "d3-transition": "^3.0.1",
    "jquery": "3.6.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.30.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^7.13.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
